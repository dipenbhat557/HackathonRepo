<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>EventManagement</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>Signinserve</servlet-name>
  <servlet-class>com.eventManager.SignIn</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Signinserve</servlet-name>
  <url-pattern>/Signinservlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>Signupserve</servlet-name>
  <servlet-class>com.eventManager.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Signupserve</servlet-name>
  <url-pattern>/Signupservlet</url-pattern>
  </servlet-mapping>
  
</web-app>